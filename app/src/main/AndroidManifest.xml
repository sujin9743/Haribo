        <activity android:name=".FindIdActivity"/>
        <activity android:name=".FindIdResultActivity"/>
        <activity android:name=".FindPwActivity"/>
        <activity android:name=".ResetPwActivity"/>
        <activity android:name=".SelectGenreActivity"/>
        <activity android:name=".SearchActivity"/>
        <activity android:name=".RecommendUserActivity"/>
        <activity android:name=".RecommendBookActivity"/>
        <activity android:name=".UserFeedActivity"/>
        <activity android:name=".ModifyProfileActivity"/>
        <activity android:name=".AlarmSettingActivity"/>
        <activity android:name=".BlockUserListActivity"/>
        <activity android:name=".FollowListActivity"/>
        <activity android:name=".DebateAddActivity"/>
        <activity android:name=".DebateDetailActivity"/>
        <activity android:name=".DebateListActivity"/>
        <activity android:name=".DebateSearchActivity"/>
        <activity android:name=".MessageViewActivity"/>
        <activity android:name=".MessageSendActivity"/>
        <activity android:name=".MRealtimeBookReport"/>
        <activity android:name=".MRealtimeBookReportSearch"/>

        <activity android:name=".MBookInfoDetail"/>
        <activity android:name=".MBookReportDetail"/>
        <activity android:name=".MBookComment"/>
        <activity android:name=".MBookWriteMain"/>
        <activity android:name=".MHashTagSearch"/>
        <activity android:name=".MBookSearch"/>
        <activity android:name=".MReviewDialog"/>


    </application>

</manifest>